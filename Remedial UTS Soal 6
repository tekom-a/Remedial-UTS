#include <iostream>
using namespace std;
int main()
{
  string bil[4][4];
  int a,b = 16
  for (int b = 0; a<4; a++)
  {
    for (int b = 0;b<4; b++)
    {
      cout<<" Matriks " <<a<<","<<b<< " = ";
      cin>> bil [a][b];
    }
  }
  for (int a=0;a<4;a++)
  {
    for (int b=0;b<4;b++)
    {
      cout<<bil[a][b]<< " | ";
    }
    cout<<endl;
  }
}
